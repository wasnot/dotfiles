[color]
	ui = true
[user]
	name = wasnot
	email = wasnot.apps@gmail.com
[alias]
	s = status
    d = diff
    b = branch
    graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
    co = checkout
	current = branch --show-current
	pull-force = ! git fetch && git reset --hard origin/$(git current)
[core]
	editor = vim
[init]
	defaultBranch = main
